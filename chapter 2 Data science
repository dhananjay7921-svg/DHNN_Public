def is_prime(n):
    if n <= 1:
        return False
    if n <= 3:
        return True

    # eliminate even numbers
    if n % 2 == 0:
        return False

    # check up to square root of n
    for i in range(3, int(n**0.5) + 1, 2):
        if n % i == 0:
            return False

    return True
